{
  "name": "deeptea-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node -r dotenv/config src/main.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . || true",
    "type-check": "tsc -p tsconfig.json || true",
    "test": "echo 'no tests yet'",
    "typeorm": "typeorm-ts-node-commonjs -d src/datasource.ts",
    "migration:generate": "yarn typeorm migration:generate src/migrations/InitSchema",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "openapi:export": "npm run build && node dist/openapi-export.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/platform-express": "^10.4.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "pg": "^8.11.3",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "helmet": "^7.0.0",
    "csurf": "^1.11.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^4.7.1",
    "nest-winston": "^1.9.7",
    "@nestjs/swagger": "^7.4.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "eslint": "^8.57.0",
    "@types/node": "^20.11.30",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.12",
    "testcontainers": "^10.9.0",
    "@testcontainers/postgresql": "^10.9.0",
    "@testcontainers/redis": "^10.9.0"
  }
}
